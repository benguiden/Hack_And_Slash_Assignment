            <string>///Alarms
var visAmount = 0;
for (i = 0; i&lt; size; i++){
    if ((alarm[i] &lt;= 0) &amp;&amp; (vis[i])){
       ///Instance Destroy
       time[i] -= 10;
       bitVelo[i] /= 2;
       if (alarm[i] &lt;= 0){
           alarm[i] = time[i];
       }
       if (time[i] &lt;= 0){
            surface_set_target(global.bloodSurface);
            //Draw Blood
             draw_sprite_ext(sprBlood, irandom(4), bitx[i], bity[i], 1, 1, 0, c_white, 0.5);
             //Draw Head
            draw_sprite(bitSpr[i], img[i], bitx[i], bity[i]);
            
            surface_reset_target();
            vis[i] = false;
       }
    }
    if (vis[i] == false){
          visAmount++;
    }
    alarm[i] --;
}

if (visAmount &gt;= size){
   instance_destroy();
}
